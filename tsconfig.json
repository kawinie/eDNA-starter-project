{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "ES2020",
        "target": "ES2020",
        "allowJs": true,
        "lib": ["ES2020", "DOM", "DOM.Iterable"],
        "jsx": "react",
        "skipLibCheck": true,
        "jsxFactory": "h",
        "jsxFragmentFactory": "Fragment",
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "components/*": ["./components/*"],
            "pages/*": ["./pages/*"],
            "styles/*": ["./styles/*"]
        }
    },
    "exclude": ["node_modules", "node_modules.nosync", "dist", "public", "build", "out"]
}
